<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="200.0" prefWidth="316.0" spacing="20.0" style="-fx-background-color: gray;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.conversor.InitController">
   <children>
      <Label text="Seleccione una opcion de conversion" />
      <MenuButton fx:id="menu" mnemonicParsing="false" text="MenuButton">
        <items>
          <MenuItem fx:id="moneda" mnemonicParsing="false" onAction="#switchMoneda" text="Moneda" />
          <MenuItem fx:id="temp" mnemonicParsing="false" onAction="#switchTemp" text="Temp" />
        </items>
      </MenuButton>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
